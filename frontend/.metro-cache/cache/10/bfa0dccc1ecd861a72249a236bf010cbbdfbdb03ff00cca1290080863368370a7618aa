{\n        const photo = await cameraRef.current.takePictureAsync({\n          base64: true,\n          quality: 0.5\n        });\n        if (photo?.base64 && token && id) {\n          await addMedia(id, 'photo', `data:image/jpeg;base64,${photo.base64}`, token, {\n            quarter: currentGame?.current_period || 1\n          });\n          Alert.default.alert('Success', 'Photo saved!');\n        }\n      } catch (error) {\n        Alert.default.alert('Error', 'Failed to take photo');\n      }\n      setShowCamera(false);\n    };\n    const handleQuarterChange = async newPeriod => {\n      if (!token || !id) return;\n      await updateGame(id, {\n        current_period: newPeriod\n      }, token);\n    };\n    const handleScoreUpdate = async () => {\n      if (!token || !id) return;\n      const newOurScore = parseInt(ourScore, 10) || 0;\n      const newOpponentScore = parseInt(opponentScore, 10) || 0;\n      await updateGame(id, {\n        our_score: newOurScore,\n        opponent_score: newOpponentScore\n      }, token);\n      setShowScoreModal(false);\n    };\n\n    // Opponent score quick buttons\n    const handleOpponentScore = async points => {\n      if (!token || !id || !currentGame) return;\n      const newOpponentScore = Math.max(0, currentGame.opponent_score + points); // Don't go below 0\n      await updateGame(id, {\n        opponent_score: newOpponentScore\n      }, token);\n    };\n\n    // Handle editing player minutes\n    const handleEditMinutes = (playerId, playerName) => {\n      const currentMinutes = playerMinutes[playerId] || 0;\n      const mins = Math.floor(currentMinutes / 60);\n      const secs = currentMinutes % 60;\n      setEditingMinutesPlayerId(playerId);\n      setEditingMinutesValue(`${mins}:${secs.toString().padStart(2, '0')}`);\n      setShowMinutesModal(true);\n    };\n    const handleSaveMinutes = () => {\n      if (!editingMinutesPlayerId) return;\n\n      // Parse the time value (format: MM:SS or just minutes)\n      let totalSeconds = 0;\n      const value = editingMinutesValue.trim();\n      if (value.includes(':')) {\n        const parts = value.split(':');\n        const mins = parseInt(parts[0]) || 0;\n        const secs = parseInt(parts[1]) || 0;\n        totalSeconds = mins * 60 + secs;\n      } else {\n        // Assume it's just minutes\n        totalSeconds = (parseInt(value) || 0) * 60;\n      }\n      setPlayerMinutes(prev => Object.assign({}, prev, {\n        [editingMinutesPlayerId]: totalSeconds\n      }));\n      setShowMinutesModal(false);\n      setEditingMinutesPlayerId(null);\n    };\n    const handleEndGame = async () => {\n      if (!token || !id || !currentGame) return;\n      // Use the actual current game scores, not the modal input state\n      await updateGame(id, {\n        status: 'completed',\n        our_score: currentGame.our_score,\n        opponent_score: currentGame.opponent_score\n      }, token);\n      setShowEndGameModal(false);\n      router.replace(`/game/summary/${id}`);\n    };\n    const openCamera = async () => {\n      if (!cameraPermission?.granted) {\n        const result = await requestCameraPermission();\n        if (!result.granted) {\n          Alert.default.alert('Permission Required', 'Camera permission is needed to take photos');\n          return;\n        }\n      }\n      setShowCamera(true);\n    };\n\n    // Live Share functionality\n    const handleShareGame = async () => {\n      if (!token || !id) return;\n      try {\n        // Generate share token from backend\n        const response = await fetch(`${API_URL}/api/games/${id}/share`, {\n          method: 'POST',\n          headers: {\n            'Authorization': `Bearer ${token}`,\n            'Content-Type': 'application/json'\n          }\n        });\n        if (!response.ok) {\n          const error = await response.json();\n          Alert.default.alert('Error', error.detail || 'Failed to create share link');\n          return;\n        }\n        const data = await response.json();\n        const shareUrl = `${API_URL}/live/${data.share_token}`;\n\n        // Show share options\n        Alert.default.alert('Share Live Game', `Anyone with this link can view the game live:\\n\\n${shareUrl}`, [{\n          text: 'Copy Link',\n          onPress: () => {\n            Clipboard.default.setString(shareUrl);\n            Alert.default.alert('Copied!', 'Share link copied to clipboard');\n          }\n        }, {\n          text: 'Share',\n          onPress: async () => {\n            try {\n              await Share.default.share({\n                message: `Watch the live game: ${currentGame?.home_team_name || 'Our Team'} vs ${currentGame?.opponent_name}\\n\\n${shareUrl}`,\n                url: shareUrl\n              });\n            } catch (e) {\n              console.error('Share error:', e);\n            }\n          }\n        }, {\n          text: 'Cancel',\n          style: 'cancel'\n        }]);\n      } catch (error) {\n        console.error('Share error:', error);\n        Alert.default.alert('Error', 'Failed to create share link');\n      }\n    };\n    if (!currentGame) {\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n        style: styles.loading,\n        children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n          style: styles.loadingText,\n          children: \"Loading game...\"\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 586,\n          columnNumber: 9\n        }, this)\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 585,\n        columnNumber: 7\n      }, this);\n    }\n\n    // Use filtered stats instead of raw stats\n    const stats = filteredStats;\n\n    // Sort players: active (in) first, then bench (out)\n    const sortedPlayerStats = [...currentGame.player_stats].sort((a, b) => {\n      const aActive = activePlayerIds.has(a.player_id);\n      const bActive = activePlayerIds.has(b.player_id);\n      if (aActive && !bActive) return -1;\n      if (!aActive && bActive) return 1;\n      return 0;\n    });\n    const inPlayers = sortedPlayerStats.filter(ps => activePlayerIds.has(ps.player_id));\n    const outPlayers = sortedPlayerStats.filter(ps => !activePlayerIds.has(ps.player_id));\n    return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n      style: styles.container,\n      children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_expoLinearGradient.LinearGradient, {\n        colors: ['#1A1A2E', '#16213E'],\n        style: styles.header,\n        children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(TouchableOpacity.default, {\n          style: styles.scoreBoard,\n          onPress: () => setShowScoreModal(true),\n          children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n            style: styles.teamScore,\n            children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n              style: styles.teamLabel,\n              children: currentGame.home_team_name?.toUpperCase() || 'YOUR TEAM'\n            }, void 0, false, {\n              fileName: _jsxFileName,\n              lineNumber: 615,\n              columnNumber: 13\n            }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n              style: styles.score,\n              children: currentGame.our_score\n            }, void 0, false, {\n              fileName: _jsxFileName,\n              lineNumber: 616,\n              columnNumber: 13\n            }, this)]\n          }, void 0, true, {\n            fileName: _jsxFileName,\n            lineNumber: 614,\n            columnNumber: 11\n          }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n            style: styles.gameInfo,\n            children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n              style: styles.quarterBadge,\n              children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(T