ayers: active (in) first, then bench (out)\n    const sortedPlayerStats = [...currentGame.player_stats].sort((a, b) => {\n      const aActive = activePlayerIds.has(a.player_id);\n      const bActive = activePlayerIds.has(b.player_id);\n      if (aActive && !bActive) return -1;\n      if (!aActive && bActive) return 1;\n      return 0;\n    });\n    const inPlayers = sortedPlayerStats.filter(ps => activePlayerIds.has(ps.player_id));\n    const outPlayers = sortedPlayerStats.filter(ps => !activePlayerIds.has(ps.player_id));\n    return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n      style: styles.container,\n      children: [isProModeGame ? /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_expoLinearGradient.LinearGradient, {\n        colors: ['#1A1A2E', '#16213E'],\n        style: styles.proModeHeader,\n        children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n          style: styles.proModeHeaderContent,\n          children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n            style: styles.proModeTitle,\n            children: \"Single Player Mode\"\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 975,\n            columnNumber: 13\n          }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n            style: styles.proModeSubtitle,\n            children: [currentGame.home_team_name, \" vs \", currentGame.opponent_name]\n          }, void 0, true, {\n            fileName: _jsxFileName,\n            lineNumber: 976,\n            columnNumber: 13\n          }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n            style: styles.proModePeriodSelector,\n            children: currentGame.period_type === 'halves' ?\n            /*#__PURE__*/\n            // Halves mode\n            (0, _reactJsxDevRuntime.jsxDEV)(_reactJsxDevRuntime.Fragment, {\n              children: [[1, 2].map(p => /*#__PURE__*/(0, _reactJsxDevRuntim